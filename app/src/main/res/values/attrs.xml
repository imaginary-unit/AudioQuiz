<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TriCheckBox">
        <attr name="state" format="integer">
            <enum name="undefined" value="-1" />
            <enum name="unchecked" value="0" />
            <enum name="checked" value="1" />
        </attr>
        <attr name="color1" format="color" />
        <attr name="color2" format="color" />
    </declare-styleable>
</resources>